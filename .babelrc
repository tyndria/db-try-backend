{
  "sourceMaps": "both",
  "presets": [
    "es2015",
    "stage-2",
    "env"
  ],
  "plugins": [
    "transform-async-to-generator",
    "transform-regenerator",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["module-resolver", {
      "root": ["./src"]
    }]
  ]
}